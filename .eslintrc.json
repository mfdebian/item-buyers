{
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/recommended",
    "react-app",
    "prettier"
  ],
  "rules": {
    "react/jsx-filename-extension": [
      "error",
      { "extensions": [".jsx", ".tsx"] }
    ],
    "import/no-anonymous-default-export": [
      "error",
      {
        "allowObject": true
      }
    ]
  }
}
